# Docker on local host
dk_host_local(){
    echo "unix:///var/run/docker.sock"
}
dk_host_local_http_proxy(){
    echo ""
}
dk_host_local_https_proxy(){
    echo ""
}
dk_host_local_no_proxy(){
    echo ""
}
